<h1>Listing customcatalogs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Features</th>
      <th>Quantity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @customcatalogs.each do |customcatalog| %>
      <tr>
        <td><%= customcatalog.name %></td>
        <td><%= customcatalog.features %></td>
        <td><%= customcatalog.quantity %></td>
        <td><%= link_to 'Show', customcatalog %></td>
        <td><%= link_to 'Edit', edit_customcatalog_path(customcatalog) %></td>
        <td><%= link_to 'Destroy', customcatalog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Customcatalog', new_customcatalog_path %>
